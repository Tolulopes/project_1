<% content_for :body do %>
  <body id='new-track'>
<% end %>

<div class="row">
<div class="small-6 small-aligned columns">

<%= simple_form_for(@track) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :description %>
      <%= f.file_field :song %>
  </div>

  <div class="form-actions">
      <%= f.hidden_field :user_id, value: current_user.id %>
      <%= f.button :submit %>
  </div>
<% end %> 
</div>
</div>